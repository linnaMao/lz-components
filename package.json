{
  "private": true,
  "name": "lz-cps",
  "version": "1.0.0",
  "scripts": {
    "start": "dumi dev",
    "build": "father-build & tsc",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "release": "npm run build && npm publish",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test"
  },
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "typings": "./lib/index.d.ts",
  "files": [
    "dist",
    "lib",
    "es",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "antd": "^4.11.2",
    "moment": "^2.29.1",
    "react": "^16.12.0"
  },
  "devDependencies": {
    "@umijs/test": "^3.0.5",
    "dumi": "^1.0.13",
    "father-build": "^1.17.2",
    "gh-pages": "^3.0.0",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "yorkie": "^2.0.0"
  }
}
